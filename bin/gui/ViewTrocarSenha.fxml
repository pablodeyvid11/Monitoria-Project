<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewControllerTrocarSenha">
   <children>
      <Button fx:id="botaoCancelar" layoutX="170.0" layoutY="361.0" mnemonicParsing="false" onAction="#acaoBotaoCancelar" text="Cancelar" />
      <Label layoutX="120.0" layoutY="53.0" text="Trocar Senha">
         <font>
            <Font name="Times New Roman" size="30.0" />
         </font>
      </Label>
      <PasswordField fx:id="senhaAntiga" alignment="CENTER" layoutX="68.0" layoutY="150.0" prefHeight="29.0" prefWidth="265.0" promptText="Senha antiga">
         <font>
            <Font size="13.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="novaSenha" alignment="CENTER" layoutX="68.0" layoutY="206.0" prefHeight="29.0" prefWidth="265.0" promptText="Nova senha">
         <font>
            <Font size="13.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="confirmarNovaSenha" alignment="CENTER" layoutX="68.0" layoutY="259.0" prefHeight="29.0" prefWidth="265.0" promptText="Confirmar senha">
         <font>
            <Font size="13.0" />
         </font>
      </PasswordField>
      <Button fx:id="botaoConfirmar" layoutX="166.0" layoutY="317.0" mnemonicParsing="false" onAction="#acaoBotaoConfirmar" text="Confirmar" />
   </children>
</AnchorPane>
